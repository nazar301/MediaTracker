<nav>
    <a href="/shows/new">Add a show</a>
</nav>

<% shows.forEach( (show, index) =>{ %>
    <h1>
        <a href="/shows/<%= show.id %>">
            <%= show.name %>
        </a>
    </h1>
    <form action='/shows/<%= show.id %>?_method=DELETE' method="POST">
        <input type="submit" value="DELETE" />
    </form>
    <a href="/shows/<%= show.id %>/edit">Edit show</a>
    <%  }) %>
    